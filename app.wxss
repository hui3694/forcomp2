/**app.wxss**/
page{
  background: #f7f7f7;
}
view{
  font-size: 16px;
  font-family: "pingfang sc medium";
}
.clear{
  clear: both;
}
.none{
  display: none;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.title{
  text-align: center;
  line-height: 45px;
}
.title image{
  width: 18px;
  vertical-align: middle;
  margin-right: 5px;
  margin-bottom: 3px;
}
.title text{
  font-weight: 500;
}