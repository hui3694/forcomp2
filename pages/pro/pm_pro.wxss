/* pages/pro/pm_pro.wxss */
.header{
  margin-top: 8px;
  margin-bottom: 15px;
  background: #fff;
}
.header .item{
  width: 33.33%;
  float: left;
  text-align: center;
}
.header .item.on{
  color: #55b737;
  border-bottom: 1px solid #55b737;
}
.header .item navigator{
  display: block;
  line-height: 36px;
}

.list .item{
  background: #fff;
  padding: 10px;
  margin-top: 8px;
}
.item .txt{
  /* width: 63%;
  float: left; */
}
.item .txt .p_title{
  font-size: 16px;
  margin-bottom: 5px;
  padding-bottom: 5px;
  border-bottom: 1px solid #eee;
}
.item .txt .p_title .status{
  float: right;
  font-size: 14px;
  color:#999;
}
.item .txt .p_zhaiyao{
  font-size: 13px;
  color: #808080;
}
.item .btn{
  text-align: right;
  margin-top: 10px;
}
.item .btn button{
  display: inline-block;
  line-height: 28px;
  font-size: 14px;
  background: none;
  margin: 0 5px;
}